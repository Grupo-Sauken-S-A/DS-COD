                        <xs:element name="ProducerEmail" type="tns:emailType" minOccurs="0"/>
                        <xs:element name="ProducerURL" type="tns:httpURIType" minOccurs="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
