                        </xs:element>
                        <xs:element name="ProducerConfidenciality" minOccurs="0">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="NO CONFIDENCIAL"/>
                                    <xs:enumeration value="CONFIDENCIAL"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="ProducerCountry" type="tns:Countries" minOccurs="0"/>
